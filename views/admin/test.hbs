
<div class="row">
    <div class="col s6 offset-s3">
        <h4 class="center">Предварительный просмотр</h4>
        <form action="#">
            <div class="section">
                {{#each question}}
                        <div class="card blue-grey darken-1 white-text">
                            <div class="card-content">
                                <p>{{number}}. {{question}}</p>
                            </div>
                            <div class="card-action">
                                <label>
                                    <input name="group{{number}}" type="radio" required/>
                                    <span class="white-text">ДА</span>
                                </label>
                                <label>
                                    <input name="group{{number}}" type="radio" />
                                    <span class="white-text">НЕТ</span>
                                </label>
                            </div>
                        </div>
                {{/each}}        
            </div>
        </form>
    </div>

    <div class="col s1 offset-s2">
             <a href="/admin/tests"><i class="material-icons medium purple-text">arrow_back</i></a>
       
    </div>
</div>


<div class="fixed-action-btn">
  <a class="btn-floating btn-large purple">
    <i class="large material-icons">mode_edit</i>
  </a>
  <ul>
    <li><a class="btn-floating red"><i class="material-icons">insert_chart</i></a></li>
    <li><a class="btn-floating yellow darken-1"><i class="material-icons">format_quote</i></a></li>
    <li><a class="btn-floating green"><i class="material-icons">publish</i></a></li>
    <li class="create-link" data-clipboard-text="192.168.0.105:3001/user/{{owner}}/30/{{code}}"><a class="btn-floating blue " onclick="M.toast({html: 'Ссылка скопирована'})" ><i class="material-icons" >attach_file</i></a></li>
  </ul>
</div>