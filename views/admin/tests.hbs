<div class="row">
  <div class="col s6 offset-s3">
    <ul class="collection">
      {{#each data}}
       <li class="collection-item"><div>{{name}}<a href="/admin/tests/{{code}}" class="secondary-content"><i class="material-icons">send</i></a></div></li>
      {{/each}}
       <li class="collection-item"><div class="center"><i class="material-icons  add-employee">add</i></div></li>
    </ul>
  </div>
</div>


  <!-- Modal Structure -->
  <div id="modal1" class="modal">
    <div class="modal-content">
      <h4>Выберите файл с компьютера</h4>
          <form action="#">
        <div class="file-field input-field">
          <div class="btn">
            <span>ФАЙЛ</span>
            <input type="file">
          </div>
          <div class="file-path-wrapper">
            <input class="file-path validate" type="text">
          </div>
        </div>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-close waves-effect waves-green btn-flat">Готово</a>
    </div>
    </form>
  </div>

<div id="modal2" class="modal">
  <div class="row">
    <div class="section">
      <div class="modal-content">
      <div class="input-field col s6 offset-s3">
      <select>
        <option value="" disabled selected>Choose your option</option>
        <option value="1">Option 1</option>
        <option value="2">Option 2</option>
        <option value="3">Option 3</option>
      </select>
      <label>Materialize Select</label>
    </div>
  </div>

  <div class="modal-footer">
    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Готово</a>
  </div>
    </div>
      
  
  </div>


</div>

<div class="fixed-action-btn">
  <a class="btn-floating btn-large purple tooltipped" data-position="left" data-tooltip="Добавить тест">
    <i class="large material-icons">create</i>
  </a>
  <ul>
    <li><a class="btn-floating green darken-1 tooltipped modal-trigger" data-position="left" data-tooltip="Загрузить" href="#modal1"><i class="material-icons">file_upload</i></a></li>
    <li><a class="btn-floating yellow darken-1 tooltipped modal-trigger" data-position="left" data-tooltip="Создать" href="#modal2"><i class="material-icons">add</i></a></li>
  </ul>
</div>